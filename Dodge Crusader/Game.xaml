<Window x:Class="Dodge_Crusader.Game"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dodge_Crusader"
        mc:Ignorable="d"
        Title="Game" Height="720" Width="720">
    <Grid>
        <Image x:Name="Hero"  Source="Hero.png" Stretch="Fill" Width="80" Height="80" Margin="0"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="467,560,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" />
    </Grid>
</Window>
